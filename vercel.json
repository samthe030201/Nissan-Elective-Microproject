{
    "version": 2,
    "builds": [
      {
        "src": "api/app1.py",
        "use": "@vercel/python"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/app1.py"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }